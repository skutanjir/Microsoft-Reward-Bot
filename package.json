{
    "name": "microsoft-rewards-automation-platform",
    "version": "1.0.0",
    "description": "Production-grade Microsoft Rewards automation platform with intelligent selector evolution and behavioral anti-detection",
    "author": "Gemini AI",
    "license": "GPL-3.0-or-later",
    "engines": {
        "node": ">=24.0.0"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc",
        "start": "node ./dist/index.js",
        "dev": "ts-node ./src/index.ts --dev",
        "test": "node --test",
        "test:unit": "node --test tests/unit/**/*.test.ts",
        "test:integration": "node --test tests/integration/**/*.test.ts",
        "test:regression": "node --test tests/regression/**/*.test.ts",
        "test:coverage": "c8 npm test",
        "test:mock-ui": "tsx tests/integration/mock-ui/run-all.ts",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "eslint \"src/**/*.ts\" --fix",
        "typecheck": "tsc --noEmit",
        "dashboard": "node ./dist/index.js --dashboard"
    },
    "keywords": [
        "Microsoft Rewards",
        "Automation",
        "Playwright",
        "Patchright",
        "Bot",
        "TypeScript",
        "Anti-Detection",
        "Machine Learning"
    ],
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.11",
        "@types/ms": "^2.1.0",
        "@types/node": "^24.10.1",
        "@types/express": "^4.17.25",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "eslint": "^9.39.1",
        "eslint-plugin-modules-newline": "^0.0.6",
        "prettier": "^3.7.1",
        "rimraf": "^6.1.2",
        "typescript": "^5.9.3",
        "c8": "^10.1.2",
        "tsx": "^4.19.2",
        "ts-node": "^10.9.2"
    },
    "dependencies": {
        "axios": "^1.13.2",
        "axios-retry": "^4.5.0",
        "better-sqlite3": "^11.8.1",
        "chalk": "^4.1.2",
        "cheerio": "^1.0.0",
        "express": "^4.21.2",
        "fingerprint-generator": "^2.1.79",
        "fingerprint-injector": "^2.1.79",
        "ghost-cursor-playwright-port": "^1.4.3",
        "http-proxy-agent": "^7.0.2",
        "https-proxy-agent": "^7.0.6",
        "ms": "^2.1.3",
        "patchright": "^1.57.0",
        "socks-proxy-agent": "^8.0.5",
        "ws": "^8.18.3",
        "zod": "^4.3.5"
    }
}